    _G.InfReflect = value
    while _G.InfReflect do wait()
        game:GetService("ReplicatedStorage").Main.Input:FireServer("Alternate", "RTZ", true)
    end
end)

MiscSection:NewToggle("AutoClick","yes",function(value)
    _G.AutoClicker = value
